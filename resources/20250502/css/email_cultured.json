{
    "body": "font-family: Roboto, sans-serif; font-size: 1.1em; text-align: center; background-color: #FAFAFA; padding: 1em; max-width: 750px; margin: 0 auto;",
    "footer": "margin-top: 2em; color: #AAAAAA; font-size: 0.7em",
    "h1": "color: #5e4296;",
    "h2": "color: #FFF; background-color: #5e4296; padding: 3px; border-radius: 0.1em;",
    "h4": "color: #666;",
    "a": "color: #00b0f0; text-decoration: none;",
    "btn" : "display: inline-block; background: #A81493; padding: 10px; color: white; text-decoration: none; border: 1px solid #A81493; border-radius: 0.3em; min-width: 150px;",
    "table": "width: 100%; border-collapse: collapse; color: #555; background-color: #FFFFFF;",
    "thead" : "color: #333; font-weight: bold; padding: 5px; background-color: #D8DF9E",
    "td" : "border: 1px solid #ddd; padding: 5px;",
    "bl_table": "width: 100%; border-collapse: collapse; color: #555;",
    "bl_thead" : "color: #333; font-weight: bold; padding: 5px; background-color: #bbdefb",
    "bl_td" : "padding: 5px;",
    "button" : "padding: 10px; background-color: #FDD327; color: #333; text-decoration: none; margin-top: 20px; border-radius: 0.25em;",
    "text_main": "color: #5e4296;",
    "text_success": "color: #28a745;",
    "text_info": "color: #03a9f4;",
    "text_danger": "color: #f44336;",
    "text_muted": "color: #999;",
    "text_center": "text-align: center;",
    "center_box_750": "max-width: 750px; margin: 0 auto;",
    "circle_avatar": "width: 40px; border-radius: 20px;",
    "thumbnail": "width: 100px; border-radius: 0.3em;"
}